ault {\n  data() {\n    return {\n      isloading: true,\n    };\n  },\n  methods: {},\n  mounted() {\n    setTimeout(() => {\n      this.isloading = false;\n    }, 2000);\n  },\n  components: {\n    loading,\n    tingLive2d,\n    tingNav,\n    tingBg,\n  },\n  computed: {\n    layout() {\n      if (this.$page.path) {\n        if (this.$frontmatter.layout) {\n          // 你也可以像默认的 globalLayout 一样首先检测 layout 是否存在\n          return this.$frontmatter.layout;\n        }\n        return \"Layout\";\n      }\n      return \"NotFound\";\n    },\n  },\n};\n</script>\n\n<style lang=\"stylus\" scoped>\n.global-layout {\n  max-width: 900px;\n  margin: 0 auto;\n  position: relative;\n}\n</style>\n"]}]}